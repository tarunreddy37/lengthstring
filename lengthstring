import java.util.*;
class lengthstring
{
public static void main(String args[])
{
	int i,n=0;
	String str;
	Scanner sc=new Scanner(System.in);
	System.out.println("enter string:");
	str=sc.nextLine();
	char ch[]=str.toCharArray();
	for(i=0;i<ch.length;i++)
	{
	if(ch[i]!=' ')
	n++;
	}
	System.out.print(n);		
}
}

